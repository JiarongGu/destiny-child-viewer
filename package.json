{
  "name": "destiny-child-viewer",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "tslint '{src,test,mocks}/**/*.{ts,tsx}' --project ./tsconfig.json",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(less)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "Destiny Child Viewer",
    "appId": "jiaronggu.destiny-child-viewer",
    "mac": {
      "category": "jiaronggu.destiny-child-viewer"
    },
    "directories": {
      "output": "release"
    },
    "extraResources": [
      {
        "from": "./resources",
        "to": "./",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:JiarongGu/destiny-child-viewer.git"
  },
  "author": {
    "name": "Jiarong Gu",
    "email": "jiaronggu@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JiarongGu/destiny-child-viewer/issues"
  },
  "homepage": "https://github.com/JiarongGu/destiny-child-viewer",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-window": "^1.8.2",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.15.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "copy-webpack-plugin": "^5.1.0",
    "cross-env": "^5.2.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.5.2",
    "css-type-loader": "^1.1.2",
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.3.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.5",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^9.0.0",
    "style-loader": "^1.1.3",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.2",
    "tslib": "^1.11.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/lowdb": "^1.0.9",
    "antd": "^3.26.15",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "deep-object-diff": "^1.1.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "lowdb": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-sink": "^0.17.0",
    "ts-memorize-decorator": "0.0.1"
  }
}
